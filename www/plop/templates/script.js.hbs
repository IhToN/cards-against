/**
 *  {{pascalCase name}} script file
**/
{{#unless functional}}
{{#if storeConnection}}
import { mapState, mapActions, mapGetters } from 'vuex'

{{#if storeModule}}
import { MODULE_NAME, ACTIONS, GETTERS } from '@/store/{{dashCase name}}'
{{else}}
import { MODULE_NAME, ACTIONS, GETTERS } from '@/store'
{{/if}}
{{/if}}
{{/unless}}

export default {
  name: '{{pascalCase name}}',
  {{#if functional}}
  funcional: true,
  {{/if}}
  {{#unless functional}}
  mixins: [],
  components: {},
  props: {},
  {{/unless}}
  {{#unless functional}}
  data () {
    return {}
  },
  {{/unless}}
  computed: {
    {{#unless functional}}
    {{#if storeConnection}}
    ...mapState(MODULE_NAME, {
      stateDefault: state => state
    }),
    ...mapGetters(MODULE_NAME, {
      gettersDefault: GETTERS.gettersDefault
    })
    {{/if}}
    {{/unless}}
  },
  methods: {
    {{#unless functional}}
    {{#if storeConnection}}
    ...mapActions(MODULE_NAME, [
      ACTIONS.actionsDefault
    ])
    {{/if}}
    {{/unless}}
  }
}
